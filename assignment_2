# Base class: Vehicle
class Vehicle:
    def move(self):
        pass  # To be implemented by subclasses

class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Sailing â›´ï¸")

# Example usage
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()  # Each class implements move() differently
